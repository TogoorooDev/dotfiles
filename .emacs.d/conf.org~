    (defun go-mode-config ()
      (go-eldoc-setup)
      (add-hook 'before-save-hook 'gofmt-before-save)
      (setq gofmt-command "goimports")
    )

  (add-hook 'go-mode-hook 'go-mode-config)
#+end_src

